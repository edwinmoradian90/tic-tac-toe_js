{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "extends": [
        "airbnb-base"
    ],
    "rules": {
        "no-shadow": "off",
        "no-param-reassign": "off",
        "eol-last": "off",
        "arrow-parens": "off",
        "no-underscore-dangle": "off",
        "no-use-before-define": "off",
        "func-names": "off",
        "prefer-destructuring": "off",
        "eqeqeq": "off"
    }
}